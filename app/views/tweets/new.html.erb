<h1>Write a new TWEET!!!!!</h1>

<%= form_for @tweet do |f| %>

  <div>
    <%= f.label :message%>
    <%= f.text_field :message%>
  </div>

  <div>
    <%= f.label :user%>
    <%= f.text_field :user%>
  </div>


<%= f.submit "Tweet that elloquent knowledge!" %>

<% end %>

<%= link_to "Show me some new tweets!", tweets_path %>
