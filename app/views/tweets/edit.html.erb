<h1>Edit Tweet</h1>

<%= form_for @tweet do |f| %>

    <div>
        <%= f.label :message %>
        <%= f.text_field :message%>
    </div>

    <div>
        <%= f.label :user %>
        <%= f.text_field :user %>
    </div>

    <p><%= f.submit "Submit changes" %></p>

<% end %>

<p><%= link_to "Back to main tweets page", tweets_path %></p>
